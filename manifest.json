{
  "manifest_version": 2,
  "name": "copy hack",
  "version": "1.0",
  "description": "csdn",
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "all_frames": true,
      "run_at": "document_start",
      "js": [
        "src/copy_hacker.js"
      ]
    },
    {
      "matches": [
        "*://blog.csdn.net/*"
      ],
      "all_frames": true,
      "js": [
        "src/click_hacker.js"
      ]
    }
  ],
  "permissions": [
    "storage",
    "tabs"
  ],
  "options_page": "src/manager.html"
}
